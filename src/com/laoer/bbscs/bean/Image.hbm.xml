<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.laoer.bbscs.bean">
  <class name="Image" table="bbscs_image" catalog="bbscs8">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="lotnoid" type="java.lang.Integer">
            <column name="lotnoid" not-null="true">
                <comment>彩种id</comment>
            </column>
        </property>
        <property name="href" type="java.lang.String">
            <column name="href">
                <comment>图片链接地址</comment>
            </column>
        </property>
        <property name="imageurl" type="java.lang.String">
            <column name="imageurl" not-null="true">
                <comment>图片地址</comment>
            </column>
        </property>
        <property name="type" type="java.lang.Integer">
            <column name="type">
                <comment>是否显示,0不显示,1显示</comment>
            </column>
        </property>
        <property name="name" type="java.lang.String">
            <column name="name" length="10">
                <comment>图片的名字</comment>
            </column>
        </property>
            <property name="inserttime" type="java.util.Date">
            <column name="inserttime" length="10">
                <comment>插入时间</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
